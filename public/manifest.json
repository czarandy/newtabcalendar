{
  "manifest_version": 2,
  "name": "New Tab Calendar",
  "version": "1.0.0",
  "minimum_chrome_version": "43",
  "description": "Customized the new tab experience to show a calendar",
  "chrome_url_overrides": {"newtab": "index.html"},
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage"]
}
